class Laptop
{
String brand;
int ram;
int hd;
double price;
public void displayAtributes()
{
System.out.println("Brand:-"+brand);
System.out.println("RAM:-"+ram);
System.out.println("Hard_disk:-"+hd);
System.out.println("Price:-"+price);
System.out.println("+++++++++++++++++++++++++++++++  ");
}
public void setAtributes(String b,int r,int d,double p)
{
brand = b;
ram = r;
hd = d;                        
price = p;
}
}